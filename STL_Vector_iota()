// Online C++ compiler to run C++ program online
#include <iostream>
#include<vector>
#include<algorithm>
#include<numeric>
using namespace std;


void show(vector<int> v11)
{
    for(int i=0;i<v11.size();i++)
    cout<<" "<<v11[i];
}



int main() {
        vector<int> v{1,0,0,0,0};
        vector<int> v2(5);
        
        iota(v.begin(),v.end(), 2 );
        cout<<"v array"<<endl;
        show(v);
      
        cout<<"\n v2 array"<<endl;
        show(v2);
        cout<<endl;
}
